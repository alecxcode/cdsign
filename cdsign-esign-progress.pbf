Global Progress_Window

Global Progress_DS, Progress_Text


Procedure OpenProgress_Window(x = 0, y = 0, width = 420, height = 100)
  Progress_Window = OpenWindow(#PB_Any, x, y, width, height, "", #PB_Window_BorderLess | #PB_Window_ScreenCentered)
  Progress_DS = ProgressBarGadget(#PB_Any, 10, 60, 400, 30, 0, 100)
  Progress_Text = TextGadget(#PB_Any, 10, 10, 400, 40, "Please, wait. Generating DS keys."+#CRLF$+"It is recommended to move mouse for entropy.", #PB_Text_Center)
EndProcedure

Procedure Progress_Window_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure


